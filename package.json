{
  "name": "agape-arc",
  "version": "0.0.0",
  "description": "A fresh new Architect project!",
  "scripts": {
    "old": "npx arc sandbox",
    "dev": "concurrently \"npm run db\" \"npm run ext\"",
    "db": "dynalite --port 5001 --path ./mydb",
    "ext": "ARC_DB_EXTERNAL=true ARC_TABLES_PORT=5001 npx arc sandbox",
    "deploy": "npx arc deploy --production"
  },
  "devDependencies": {
    "@architect/architect": "^10.10.0",
    "concurrently": "^7.6.0",
    "dynalite": "^3.2.2"
  }
}